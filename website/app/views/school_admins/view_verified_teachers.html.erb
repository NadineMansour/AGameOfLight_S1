<div class="row">
<div class="col-md-2">
<%= render partial: 'menu_school_admin' %>
</div>
<div class="col-md-8">
<table class="table table-hover">
<thead>
<tr>
<th>Name</th>
<th>School</th>
<th>Remove</th>
<th colspan="3"></th>
</tr>
</thead>
<tbody>
<% @teachers.each do |teacher| %>
<tr>
<td> <%= teacher[:teacher_name] %> </td>
<td> <%= teacher[:school] %></td>
<td>
<%= form_tag("/school_admins/" +
"/remove_verified_teacher/" + teacher[:id].to_s, method: :put) do %>
<%= button_tag(type: "submit", class: "btn btn-primary") do %>
Remove
<% end %>
<% end %>
</td>

</tr>
<% end %>
</tbody>
</table>
</div>
</div>
